--source include/no_ps_protocol.inc
--source include/have_null_audit_plugin.inc

--echo #
--echo # Bug#35820314 buffer overflow in default install plugin
--echo #
let $overflow_text = '




EEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE';

--replace_regex /\.dll/.so/
eval INSTALL PLUGIN null_audit SONAME '$AUDIT_NULL';
INSTALL COMPONENT "file://component_test_audit_api_message";

eval SELECT test_audit_api_message_replace(0, $overflow_text) AS 'long component';
eval SELECT test_audit_api_message_replace(1, $overflow_text) AS 'long producer';
eval SELECT test_audit_api_message_replace(2, $overflow_text) AS 'long message';
eval SELECT test_audit_api_message_replace(3, $overflow_text) AS 'long key';
eval SELECT test_audit_api_message_replace(4, $overflow_text) AS 'long value';
eval SELECT test_audit_api_message_replace(5, $overflow_text) AS '1. parameter too big';
eval SELECT test_audit_api_message_replace(-1, $overflow_text) AS '1. parameter too small';
SELECT test_audit_api_message_replace() AS 'no parameters';

UNINSTALL COMPONENT "file://component_test_audit_api_message";
UNINSTALL PLUGIN null_audit;