exec $MYSQLXTEST
  -u x_root --password='' -h127.0.0.1 --no-auth
  --file=$MYSQL_TMP_DIR/capabilities_compression_algorithm_set.xpl 2>&1
  -v%VALUE%=wrong_name -v%RESULT%=Mysqlx.Error;

